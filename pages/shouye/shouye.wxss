/**index.wxss**/

.container{
  height: 1500rpx;
}
.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: #2A8CE5;
  display: inline-flex
}
.container .search-left{
  flex: 9;
  background: #4EA3E7;
  text-align: left;
}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 26rpx;
}
.search-placeholder{
  color: #8CCEFD;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 40rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 15rpx;
}
.container .search .search-right{
  flex: 0.8;
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}

.lunbo{
  width: 100%
}
.lunbo image{
  width: 100%;
  height: 100%
}

.zhuanlan-1 image{
  width: 110rpx;
  height: 110rpx;
}
.zhuanlan-1{
  display: flex;
  flex-direction: row
}
.zhuanlan-1 view{
  margin: 40rpx
}
.zhuanlan-1 text{
  display: block;
  font-size: 27rpx;
  color: gray;
  font-weight: 500
}

.zhuanlan-2 image{
  width: 110rpx;
